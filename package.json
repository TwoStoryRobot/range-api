  "scripts": {
    "start": "node build/server",
    "debug": "node --inspect=0.0.0.0:9222 --nolazy build/server",
    "dev": "cross-env NODE_ENV=development nodemon server --exec babel-node --inspect",
    "build": "cross-env NODE_ENV=production gulp",
    "build:doc": "./node_modules/.bin/apidoc -i server/ -o doc/api",
    "test": "cross-env NODE_ENV=test jest __tests__",
    "test:watch": "cross-env NODE_ENV=test jest __tests__ --watch",
    "test:security": "nsp check",
    "test:lint": "eslint --env node --ext .js server",
    "db:migrate": "sequelize --url $POSTGRES_URL db:migrate",
    "db:seed": "sequelize --url $POSTGRES_URL db:seed:all"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:bcgov/secure-image-api.git"
  },
  "@std/esm": {
    "esm": "all",
